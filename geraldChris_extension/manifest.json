{
    "name": "Jobx To-Do List",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/gc-logo-128.png"
    },
    "permissions": [
      "storage"
    ],
    "content_scripts":[
     {
      "matches": [
        "<all_urls>"
      ],
      "js":[
        "contentScript.js"
      ]
     }
    ]
    
  }